{
    "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
    "version": 1,
    "newProjectRoot": "projects",
    "projects": {
        "mxMaintainer": {
            "root": "",
            "sourceRoot": "src",
            "projectType": "application",
            "prefix": "app",
            "schematics": {
                "@schematics/angular:component": {
                    "styleext": "scss"
                }
            },
            "architect": {
                "build": {
                    "builder": "@angular-devkit/build-angular:browser",
                    "options": {
                        "outputPath": "dist",
                        "index": "src/index.html",
                        "main": "src/main.ts",
                        "polyfills": "src/polyfills.ts",
                        "tsConfig": "src/tsconfig.app.json",
                        "assets": [
                            "src/favicon.ico",
                            "src/assets"
                        ],
                        "styles": [
                            "src/styles.scss",
                            "src/assets/icons/material/material-icons.css",
                            "node_modules/primeicons/primeicons.css",
                            "node_modules/primeng/resources/themes/nova-light/theme.css",
                            "node_modules/primeng/resources/primeng.min.css"
                        ],
                        "scripts": [],
                        "showCircularDependencies": false
                    },
                    "configurations": {
                        "development": {
                            "baseHref": "/",
                            "fileReplacements": [
                                {
                                    "replace": "src/environments/environment.ts",
                                    "with"   : "src/environments/environment.dev.ts"
                                },
                                {
                                    "replace": "src/app/data/repos/sp-repo.service.ts",
                                    "with"   : "src/app/data/repos/sp-repo-dev.service.ts"
                                },
                                {
                                    "replace": "src/app/data/repos/base-spjsom-repo.service.ts",
                                    "with"   : "src/app/data/repos/base-spjsom-repo-dev.service.ts"
                                }
                            ],
                            "optimization": false,
                            "outputHashing": "all",
                            "sourceMap": true,
                            "extractCss": true,
                            "namedChunks": true,
                            "aot": false,
                            "extractLicenses": true,
                            "vendorChunk": false,
                            "buildOptimizer": false
                        },
                        "production": {
                            "baseHref": "/sites/10918/mx-maintainer/",
                            "fileReplacements": [
                                {
                                    "replace": "src/environments/environment.ts",
                                    "with": "src/environments/environment.prod.ts"
                                }
                            ],
                            "optimization": true,
                            "outputHashing": "all",
                            "sourceMap": false,
                            "extractCss": true,
                            "namedChunks": false,
                            "aot": true,
                            "extractLicenses": true,
                            "vendorChunk": false,
                            "buildOptimizer": true
                        },
                        "sharepointDev": {
                            "baseHref": "/sites/10918/mx-maintainer/",
                            "fileReplacements": [
                                {
                                    "replace": "src/environments/environment.ts",
                                    "with": "src/environments/environment.share.ts"
                                }
                            ],
                            "optimization": false,
                            "outputHashing": "none",
                            "sourceMap": true,
                            "extractCss": false,
                            "namedChunks": true,
                            "aot": false,
                            "extractLicenses": false,
                            "vendorChunk": true,
                            "buildOptimizer": false
                        },
                        "ec": {
                            "sourceMap": true,
                            "extractCss": true
                        },
                        "hmr": {
                            "fileReplacements": [
                                {
                                    "replace": "src/environments/environment.ts",
                                    "with": "src/environments/environment.hmr.ts"
                                }
                            ]
                        }
                    }
                },
                "serve": {
                    "builder": "@angular-devkit/build-angular:dev-server",
                    "options": {
                        "browserTarget": "mxMaintainer:build"
                    },
                    "configurations": {
                        "development": {
                            "browserTarget": "mxMaintainer:build:development"
                        },
                        "production": {
                            "browserTarget": "mxMaintainer:build:production"
                        },
                        "hmr": {
                            "hmr": true,
                            "browserTarget": "mxMaintainer:build:hmr"
                        },
                        "ec": {
                            "browserTarget": "mxMaintainer:build:ec"
                        }
                    }
                },
                "extract-i18n": {
                    "builder": "@angular-devkit/build-angular:extract-i18n",
                    "options": {
                        "browserTarget": "mxMaintainer:build"
                    }
                },
                "test": {
                    "builder": "@angular-devkit/build-angular:karma",
                    "options": {
                        "main": "src/test.ts",
                        "polyfills": "src/polyfills.ts",
                        "tsConfig": "src/tsconfig.spec.json",
                        "karmaConfig": "src/karma.conf.js",
                        "styles": [
                            "styles.scss"
                        ],
                        "scripts": [],
                        "assets": [
                            "src/favicon.ico",
                            "src/assets"
                        ]
                    }
                },
                "lint": {
                    "builder": "@angular-devkit/build-angular:tslint",
                    "options": {
                        "tsConfig": [
                            "src/tsconfig.app.json",
                            "src/tsconfig.spec.json"
                        ],
                        "tslintConfig": "src/tslint.json",
                        "exclude": [
                            "**/node_modules/**",
                            "**/@fuse/**"
                        ]
                    }
                }
            }
        },
        "mxMaintainer-e2e": {
            "root": "e2e/",
            "projectType": "application",
            "architect": {
                "e2e": {
                    "builder": "@angular-devkit/build-angular:protractor",
                    "options": {
                        "protractorConfig": "e2e/protractor.conf.js",
                        "devServerTarget": "mxMaintainer:serve"
                    },
                    "configurations": {
                        "production": {
                            "devServerTarget": "mx-maintainer:serve:production"
                        }
                    }
                },
                "lint": {
                    "builder": "@angular-devkit/build-angular:tslint",
                    "options": {
                        "tsConfig": "e2e/tsconfig.e2e.json",
                        "exclude": [
                            "**/node_modules/**"
                        ]
                    }
                }
            }
        }
    },
    "defaultProject": "mx-maintainer",
    "schematics": {
        "@schematics/angular:component": {
            "styleext": "scss"
        }
    }
}