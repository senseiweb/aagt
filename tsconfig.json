{
    "compileOnSave": false,
    "compilerOptions": {
        "baseUrl": "./src",
        "outDir": "./dist/out-tsc",
        "sourceMap": true,
        "declaration": false,
        "moduleResolution": "node",
        "emitDecoratorMetadata": true,
        "experimentalDecorators": true,
        "downlevelIteration": true,
        "paths": {
            "breeze": ["node_modules/breeze-client/breeze-debug"],
            "@fuse": ["@fuse/"],
            "@ctypes": ["@ctypes/"],
            "@odata": ["@odata/"],
            "sp-jsom": ["node_modules/@microsoft/sp-jsom"]
        },
        "target": "es5",
        "typeRoots": ["node_modules/@types"],
        "lib": ["es2017", "dom"]
    }
}
