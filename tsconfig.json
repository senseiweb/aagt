{
    "compileOnSave": false,
    "compilerOptions": {
        "baseUrl": "./src",
        "outDir": "./dist/out-tsc",
        "sourceMap": true,
        "declaration": false,
        "moduleResolution": "node",
        "emitDecoratorMetadata": true,
        "experimentalDecorators": true,
        "paths": {
            "@fuse": [
                "@fuse/"
            ],
            "sp-jsom": [
                "node_modules/@microsoft/sp-jsom"
            ]
        },
        "target": "es5",
        "typeRoots": [
            "node_modules/@types",
            "node_modules/breeze-client/src",
            "src/@types"
        ],
        "lib": [
            "es2017",
            "dom"
        ]
    }
}
